{
  "name": "scheduled-meets",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "webpack build",
    "codegen": "graphql-codegen --config codegen.config.ts",
    "codegen:watch": "graphql-codegen --config codegen.config.ts --watch",
    "start": "concurrently \"graphql-codegen --config codegen.config.ts --watch\" \"webpack serve\"",
    "start:open": "concurrently \"graphql-codegen --config codegen.config.ts --watch\" \"webpack serve --open-app-name firefox\""
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.13.6",
    "@graphql-codegen/client-preset": "^1.0.6",
    "@graphql-codegen/introspection": "2.2.1",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.27.1",
    "html-webpack-plugin": "^5.5.0",
    "prettier": "2.5.0",
    "ts-loader": "^9.4.1",
    "typescript": "^4.8.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.6.0"
  },
  "dependencies": {
    "@apollo/client": "^3.5.6",
    "@reduxjs/toolkit": "^1.8.6",
    "@types/node": "^12.20.37",
    "@types/react": "^17.0.36",
    "@types/react-dom": "^17.0.11",
    "dotenv": "^16.0.2",
    "graphql": "^16.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^8.0.4",
    "react-router-dom": "^6.4.0",
    "react-ternary-operator": "^0.0.4",
    "react-when-then": "^0.1.16",
    "redux-persist": "^6.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
