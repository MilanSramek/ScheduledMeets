{
  "name": "scheduled-meets",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "locales": "i18next --config i18next-parser.config.json",
    "codegen": "graphql-codegen --config codegen.config.ts",
    "build": "webpack build",
    "codegen:watch": "graphql-codegen --config codegen.config.ts --watch",
    "start": "concurrently \"graphql-codegen --config codegen.config.ts --watch\" \"webpack serve\"",
    "start:open": "concurrently \"graphql-codegen --config codegen.config.ts --watch\" \"webpack serve --open-app-name firefox\"",
    "start:server": "cd ../server/src/ScheduledMeets && dotnet run"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.13.12",
    "@graphql-codegen/client-preset": "^1.1.4",
    "@graphql-codegen/introspection": "^2.2.1",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react": "^7.31.11",
    "html-webpack-plugin": "^5.5.0",
    "prettier": "^2.7.1",
    "ts-loader": "^9.4.1",
    "typescript": "^4.9.3",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.0",
    "webpack-dev-server": "^4.11.1"
  },
  "dependencies": {
    "@apollo/client": "^3.7.1",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/icons-material": "^5.10.14",
    "@mui/material": "^5.10.14",
    "@reduxjs/toolkit": "^1.9.0",
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.9",
    "dotenv": "^16.0.3",
    "graphql": "^16.6.0",
    "i18next": "^22.0.6",
    "i18next-browser-languagedetector": "^7.0.1",
    "i18next-http-backend": "^2.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^12.0.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.4.3",
    "react-ternary-operator": "^0.0.4",
    "react-when-then": "^0.1.16",
    "redux-persist": "^6.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
